<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <EditTextPreference
        android:defaultValue="Not set"
        android:key="pref_first_name"
        android:summary="Set First Name"
        android:title="First Name"></EditTextPreference>
    <EditTextPreference
        android:defaultValue="Not set"
        android:key="pref_last_name"
        android:summary="Set Last Name"
        android:title="Last Name"></EditTextPreference>
    <EditTextPreference
        android:defaultValue="Not set"
        android:key="pref_dob"
        android:summary="Set Date of birth"
        android:title="Date of Birth"></EditTextPreference>
    <EditTextPreference
        android:defaultValue="Not set"
        android:enabled="false"
        android:key="pref_email"
        android:summary="Set Email"
        android:title="Email"></EditTextPreference>
</PreferenceScreen>